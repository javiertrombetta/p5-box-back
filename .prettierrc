{
	"useTabs": true,
	"tabWidth": 2,
	"semi": true,
	"singleQuote": true,
	"printWidth": 180,
	"jsxSingleQuote": false,
	"trailingComma": "all",
	"bracketSpacing": true,
	"bracketSameLine": false,
	"arrowParens": "avoid",
	"importOrder": ["^[./]"],
	"importOrderSortSpecifiers": true,
	"importOrderSeparation": true
}
